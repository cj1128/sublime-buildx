%YAML 1.2
---
name: BuildX
scope: text.buildoutput
contexts:
  main:
    # [Finished in 0.1s]
    - match: '^\[(Finished) in (.+s)\]$'
      captures:
        1: success_msg
        2: success_time

    # [Finished in 0.1s with exit code 1]
    - match: '^\[(Finished) in (.+s) with exit code (\d+)\]$'
      captures:
        1: error_msg
        2: error_time
        3: error_code
